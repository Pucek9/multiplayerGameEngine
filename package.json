    "@types/three": "^0.103.2",
    "autoprefixer": "^9.7.4",
    "awesome-typescript-loader": "^5.2.1",
    "css-loader": "^3.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "ip": "^1.1.5",
    "node-sass": "^4.13.0",
    "nodemon": "^1.19.4",
    "nodemon-webpack-plugin": "^4.1.1",
